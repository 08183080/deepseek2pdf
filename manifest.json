{
    "manifest_version": 3,
    "name": "deepseek2pdf",
    "version": "1.0",
    "description": "将和deepseek的聊天内容有价值的信息导出pdf文件",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }